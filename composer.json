{
    "type": "project",
    "license": "mit",
    "name": "coop/coop",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "babdev/pagerfanta-bundle": "^2.8",
        "borsaco/telegram-bot-api-bundle": "*",
        "doctrine/doctrine-bundle": "^2.1",
        "erusev/parsedown": "^1.7",
        "gedmo/doctrine-extensions": "^2.4",
        "knplabs/knp-menu-bundle": "^3.0",
        "liip/imagine-bundle": "*",
        "nelmio/cors-bundle": "^2.0",
        "sensio/framework-extra-bundle": "*",
        "smart-core/core-bundle": "dev-master",
        "smart-core/db-dumper-bundle": "dev-master",
        "sonata-project/intl-bundle": "*",
        "stfalcon/tinymce-bundle": "*",
        "symfony/asset": "5.2.*",
        "symfony/cache": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/maker-bundle": "^1.12",
        "symfony/orm-pack": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/templating": "5.2.*",
        "symfony/translation": "5.2.*",
        "symfony/validator": "5.2.*",
        "symfony/yaml": "5.2.*",
        "twig/extensions": "^1.5",
        "vich/uploader-bundle": "*"
    },
    "require-dev": {
    },
    "config": {
        "apcu-autoloader": true,
        "component-dir": "public/components",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "SmartCore\\Bundle\\MediaBundle\\": "src-vendor/MediaBundle/",
            "SmartCore\\Bundle\\TexterBundle\\": "src-vendor/TexterBundle/",
            "Coop\\JointPurchaseBundle\\": "src-vendor/JointPurchaseBundle/",
            "Borsaco\\TelegramBotApiBundle\\": "src-overload/Borsaco/TelegramBotApiBundle/",
            "Gedmo\\": "src-overload/Gedmo"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "sh bin/clear_cache": "script",
            "rm -rf public/bundles": "script",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "sh bin/init_dirs": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.2.*"
        }
    }
}
