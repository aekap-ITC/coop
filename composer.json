{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "borsaco/telegram-bot-api-bundle": "*",
        "doctrine/doctrine-bundle": "*",
        "knplabs/knp-menu-bundle": "*",
        "liip/imagine-bundle": "dev-master",
        "sensio/framework-extra-bundle": "*",
        "smart-core/core-bundle": "dev-master",
        "smart-core/db-dumper-bundle": "dev-master",
        "sonata-project/intl-bundle": "*",
        "symfony/asset": "4.4.*",
        "symfony/cache": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/maker-bundle": "^1.12",
        "symfony/orm-pack": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/templating": "4.4.*",
        "symfony/translation": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/yaml": "4.4.*",
        "vich/uploader-bundle": "*",
        "white-october/pagerfanta-bundle": "*"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "SmartCore\\Bundle\\MediaBundle\\": "src-vendor/MediaBundle/",
            "Coop\\JointPurchaseBundle\\": "src-vendor/JointPurchaseBundle/",
            "Borsaco\\TelegramBotApiBundle\\": "src-overload/Borsaco/TelegramBotApiBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "sh bin/clear_cache": "script",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "sh bin/init_var": "script",
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
